{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "semicolon": [
            true,
            "always"
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "no-console": [
            false
        ],
        "member-access": false,
        "object-literal-sort-keys": false
    }
}